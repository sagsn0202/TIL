    "num1 = 42\n",
    "num2 = 14\n",
    "\n",
    "temp_num1 = num1\n",
    "temp_num2 = num2\n",
    "\n",
    "while temp_num2 != 0:\n",
    "    rem = temp_num1 % temp_num2\n",
    "    temp_num1 = temp_num2\n",
    "    temp_num2 = rem\n",
    "\n",
    "#최대공약수\n",
    "gcd = temp_num1\n",
    "#최소공배수\n",
    "lcm = int((num1 * num2) / gcd)\n",
    "\n",
    "print(gcd, lcm)"
   ]
