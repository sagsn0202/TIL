    "# 1\n",
    "from flask import Flask\n",
    "\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route('/dictionary/<string:word>')\n",
    "def dictionary(word):\n",
    "    words_dict = { 'apple': '사과' }\n",
    "    if word in words_dict:\n",
    "        return f'{word}은(는) {words_dict[word]}'\n",
    "    else:\n",
    "        return f'{word}은(는) 나만의 단어장에 없는 단어입니다.'"
   ]
  }
